@* The filename needs to be the same as the component's alias. We get the markup for this file from the header.cshtml file.
    Then we need to change the HomePage.cshtml template to call this instead of the header.cshtml file.
*@
@* Generated Models->RichText.generated.cs.*@
@inherits UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockGridItem<ContentModels.RichText>>//regenerate the models! 

<div class="container my-5">
    <div class="col-lg-8 mx-auto"">
        @if (Model.Content.Text != null)
        {
            @Model.Content.Text
        } 
        else {
                <p>To lazy to type a real error message</p>
        }
    </div>
</div>



